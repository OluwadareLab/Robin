{
  "compilerOptions": {
    "noImplicitAny": false,
    "skipLibCheck": true,
    "module": "esnext",
    "target": "esnext",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution":"node",  
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "files": ["src/**/*"],
  "include": ["src/**/*"]
}
